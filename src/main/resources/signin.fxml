<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="650.0"
            prefWidth="1060.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="main.Controller">
    <MenuBar prefHeight="24.0" prefWidth="700.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem id="openFolderMenuButton" fx:id="openFolderMenuButton" mnemonicParsing="false"
                      text="Open Folder"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Edit">
            <MenuItem fx:id="forceSignInOutMenuItem" mnemonicParsing="false" text="Force Sign In/Out"/>
            <MenuItem fx:id="addMenuItem" mnemonicParsing="false" text="Add "/>
            <MenuItem fx:id="editMenuItem" mnemonicParsing="false" text="Edit"/>
            <MenuItem fx:id="deleteMenuItem" mnemonicParsing="false" text="Delete"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About"/>
        </Menu>
    </MenuBar>
    <TabPane layoutY="75.0" prefHeight="325.0" prefWidth="600.0" tabClosingPolicy="UNAVAILABLE"
             AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
             AnchorPane.topAnchor="75.0">
        <Tab fx:id="checkedInTab" closable="false" text="Checked In">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                <TableView fx:id="CheckinTable" editable="true" prefHeight="351.0" prefWidth="600.0"
                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="CIDColumn" prefWidth="126.0" text="ID"/>
                        <TableColumn fx:id="CNameColumn" prefWidth="195.0" text="Name"/>
                        <TableColumn fx:id="CTimestampColumn" prefWidth="141.0" text="Timestamp"/>
                        <TableColumn fx:id="CCertificationsColumn" prefWidth="266.0" text="Certifications"/>
                        <TableColumn prefWidth="120.6666259765625" text="Strikes"/>
                        <TableColumn fx:id="CNotesColumn" minWidth="0.0" prefWidth="336.0" text="Notes"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab fx:id="directoryTab" closable="false" text="Directory">
            <AnchorPane>
                <TableView fx:id="DirectoryTable" editable="true" prefHeight="351.0" prefWidth="600.0"
                           AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
                           AnchorPane.topAnchor="0.0">
                    <columns>
                        <TableColumn fx:id="DIDColumn" prefWidth="91.0" text="ID"/>
                        <TableColumn fx:id="DNameColumn" prefWidth="168.0" text="Name"/>
                        <TableColumn fx:id="DEmailColumn" prefWidth="144.0" text="Email"/>
                        <TableColumn fx:id="DCertificationsColumn" prefWidth="232.66668701171875"
                                     text="Certifications"/>
                        <TableColumn prefWidth="120.6666259765625" text="Strikes"/>
                        <TableColumn fx:id="DNotesColumn" minWidth="0.0" prefWidth="225.3333740234375"
                                     text="Notes"/>
                    </columns>
                    <columnResizePolicy>
                        <TableView fx:constant="CONSTRAINED_RESIZE_POLICY"/>
                    </columnResizePolicy>
                </TableView>
            </AnchorPane>
        </Tab>
        <Tab text="Log">
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0"/>
        </Tab>
    </TabPane>
    <AnchorPane layoutY="25.0" prefHeight="50.0" prefWidth="700.0" AnchorPane.leftAnchor="0.0"
                AnchorPane.rightAnchor="0.0">
        <Button fx:id="signInButton" layoutX="259.0" layoutY="15.0" maxHeight="-Infinity" maxWidth="-Infinity"
                minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" text="Sign In/Out"/>
        <TextField fx:id="idField" layoutX="18.0" layoutY="13.0" prefHeight="29.0" prefWidth="207.0"
                   promptText="Swipe ID"/>
    </AnchorPane>
</AnchorPane>
